//matrix pattern

#include<stdio.h>
void main()
{
    int i,j,rows,col;
    printf("Enter no.of rows:");
    scanf("%d",&rows);
    printf("Enter no.of col:");
    scanf("%d",&col);
    for(i=1;i<=rows;i++)
    {
        for(j=1;j<=col;j++)
        {
            printf("*");
        }
        printf("\n");
    }
}

Output:
Enter no.of rows:5
Enter no.of col:6
******
******
******
******
******
