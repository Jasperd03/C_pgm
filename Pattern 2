/*Write the code for the following pattern....if n=6

  ****
  ****
****
****
  ****
  ****

*/

#include<stdio.h>
int main()
{
	int rows=6,i,j,k,a,b,c_rows;
	c_rows=rows/2;
	for(i=1;i<=c_rows;i++)
	{
		if(i%2==1)
		{
			for(a=1;a<=2;a++)
			{
				printf("  ");
				for(j=1;j<=4;j++)
				{
					printf("*");
				}
				printf("\n");
				j=1;
			}
			a=1;
		}
		else
		{
			for(b=1;b<=2;b++)
			{
				for(k=1;k<=4;k++)
				{
					printf("*");
				}
				printf("\n");
				k=1;
			}
			b=1;
		}
	}
}
